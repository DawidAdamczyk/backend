{% extends "base.html.twig" %}

{% block body %}
  {{ form_start(form) }}
  <div class="form-group mt-4">
    {{ form_row(form.name, {'attr': {'class': 'form-control mt-2'}}) }}
    {{ form_row(form.info, {'attr': {'class': 'form-control mt-2'}}) }}
    {{ form_row(form._token) }}
     <button class="mt-2" type="submit">Add!</button>
  </div>
{% endblock %}